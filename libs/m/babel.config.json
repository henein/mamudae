{
  "presets": ["@babel/preset-env", "@babel/preset-typescript"],
  "plugins": [
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    ["@babel/plugin-proposal-class-properties", { "loose" : false }],
    "@babel/proposal-object-rest-spread",
    [
      "@babel/transform-runtime", {
      "corejs": 3
      }
    ]
  ]
}
