{
  "name": "maple-draft-pick",
  "version": "0.1.0",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc -p tsconfig.server.json && webpack --config webpack.prod.js",
    "dev": "npm run clean && concurrently --kill-others \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "tsc-watch -p tsconfig.server.json --onSuccess \"node dist/server\"",
    "dev:client": "webpack --watch --config webpack.dev.js",
    "test": "jest"
  },
  "author": "Pdom",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/runtime-corejs3": "^7.11.2",
    "@types/clean-webpack-plugin": "^0.1.3",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.14",
    "@types/socket.io": "^2.1.11",
    "@types/socket.io-client": "^1.4.34",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.3.0",
    "core-js": "3",
    "css-loader": "^4.3.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-webpack-plugin": "^2.1.0",
    "file-loader": "^6.1.1",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.5.2",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.0.3",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-merge": "^5.2.0"
  },
  "dependencies": {
    "express": "^4.17.1",
    "mobx": "^6.0.1",
    "pixi.js": "^5.3.3",
    "socket.io": "^2.3.0"
  }
}
